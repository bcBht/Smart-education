.ct-list {
  margin: 20rpx 0;
  margin-top: 120rpx;
}

.ct-list__tip {

}

.ct-list__item {
  background-color: white;
  margin: 20rpx 0;
  padding: 20rpx;
}

.ct-list__item-top {
  display: flex;
  margin: 40rpx 0;
}

.ct-list__item-car {
  height: 48rpx;
  width: 48rpx;
}

.ct-list__item-title {
  width: 100rpx;
}

.ct-list__title-mt {
  margin: 0 20rpx;
}

.ct-list__item-collect {  
  width: 180rpx;
  margin: 0 50rpx;
}

.ct-list__item-collected {
  color: blue;
}

.ct-list__item-collect image {
  height: 32rpx;
  width: 32rpx;
  margin-right: 8rpx;
}

.ct-list__item-fill {
  width: 44rpx;
}

.ct-list__item-left {
  height: 24rpx;
  width: 24rpx;
  justify-self: flex-end;
}

.ct-list__item-bottom {
  display: flex;
}

.ct-list__item-line {

  height: 2px;
  width: 74rpx;
  background: #4C4C4C;
  position: relative;
  top: 90rpx;
  margin-bottom: 16rpx;
  
}

.ct-list__item-line::after {
  content: '';
  display: block;
  width: 18rpx;
  height: 2px;
  position: absolute;
  right: 0;
  bottom: 8rpx;
  transform: rotate(45deg);
  background: #4C4C4C;
}

.ct-list__item-area {
  /* display: flex; */
  flex: 1;
}

.ct-list__item-area-two {
  text-align: right;
}

.ct-list__item-start {
  background-color: green;
  width: 1.4em;
  text-align: center;
  border-radius: 25%;
  color: white;
}

.ct-list__item-end {
  background-color: red;
  width: 1.4em;
  text-align: center;
  border-radius: 25%;
  color: white;
  position: relative;
  left: 240rpx;
}

.ct-list__item-area-title {
  margin: 10rpx 0;
}

.radio_view{
  margin: 100rpx 0 0 0;
}

.bg_radio{
  background-color: #F6FAFF;
}
.modle_btn {
  display: flex;
}

.modle_btn button {
  background-color: #F6FAFF;
  color: black;
  width: 50%;
}

.modle_btn button:nth-child(2) {
  color: blue;
}