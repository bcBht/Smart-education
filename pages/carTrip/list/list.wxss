.ct-container {
  position: relative;
  padding: 0 32rpx;
}

.ct-select {
  position: fixed;
  top: 20rpx;
  left: 32rpx;
  background: #fff;
  display: flex;
  width: 686rpx;
  height: 96rpx;
  z-index: 100;
}

.ct-select__item {
  width: 343rpx;
  text-align: center;
  font-size: 24rpx;
  color: #333;
  display: flex;
  align-items: center;
  justify-content: center;
}

.ct-select__item-title {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  height: 96rpx;
  line-height: 96rpx;
  max-width: 283rpx;
}

.ct-select__item-column {
  width: 0;
  height: 0;
  border: 10rpx solid transparent;
  border-top: 12rpx solid #333;
  margin-left: 14rpx;
  margin-top: 10rpx;
}

